package Assignment1;

public class ArmstrongNumber {

	public static void main(String[] args) {		
	
		int x=0,n=0;
		
		for(int i=100;i<102;i++) {
			int temp=i;
		  while (temp>0) {
		  x=temp%10;
		  n=x*x*x+n;
		  temp=temp/10;
		 
		
				}
		  System.out.println(n);
//		  if(n==i) {
//			System.out.println(i);  
//		  }
//		
		
	}
	}

}
